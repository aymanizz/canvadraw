    },
    "devDependencies": {
        "browser-sync": "^2.26.7",
        "browserify": "^16.5.1",
        "concurrently": "^5.2.0",
        "tsify": "^4.0.1",
        "typescript": "^3.9.3",
        "watchify": "^3.11.1"
    },
    "dependencies": {
        "konva": "^6.0.0"
    }
